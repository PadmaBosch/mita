fn returnOptional0(): int32? {
	var res: int32? = 1;
	return res;
}
fn returnOptional1(): int32? {
	var res: int32? = none();
	return res;
}
fn returnOptional2(): int32? {
	var res: int32? = some(1);
	return res;
}
fn returnOptional3(): int32? {
	return 1;
}
fn returnOptional4(): int32? {
	return none();
}
fn returnOptional5(): int32? {
	return some(1);
}

